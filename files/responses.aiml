<?xml version="1.0" encoding="UTF-8"?>
<aiml version="2.0">
	<topic name="questions">
	    	    <category>
	        <pattern>^ <set>yes</set> ^</pattern>
	        <template>
	            <condition name="question">
	                <!-- Do you like to create things you can see? -->
	                <li value="1">
	                    I'm a visual person too.
	                    <think>
                            <set name="frontend">
                                <map name="successor"><get name="frontend" /></map>
                            </set>
                        </think>
	                    <srai>QUESTION</srai>
	                </li>
	                <!-- Do you like solving complex problems? -->
	                <li value="2">
	                    That's good! That will help you no matter what field you choose.
	                    <think>
                            <set name="backend">
                                <map name="successor"><get name="backend" /></map>
                            </set>
                            <set name="ds">
                                <map name="successor"><get name="ds" /></map>
                            </set>
                        </think>
	                    <srai>QUESTION</srai>
	                </li>
	                <!-- Do you mind not having a clear path to solving the problems given to you? -->
	                <li value="3">
	                    Good for you! Personally, I love clear instructions, but I'm glad that works for you.
	                    <think>
                            <set name="pm">
                                <map name="successor"><get name="pm" /></map>
                            </set>
                            <set name="sec">
                                <map name="successor"><get name="sec" /></map>
                            </set>
                        </think>
	                    <srai>QUESTION</srai>
	                </li>
	                <!-- Do you like working with people? -->
	                <li value="4">
	                    Me too! That's all I do actually...
	                    <think>
                            <set name="pm">
                                <map name="successor"><get name="pm" /></map>
                            </set>
                            <set name="sec">
                                <map name="successor"><get name="sec" /></map>
                            </set>
                            <set name="ds">
                                <map name="successor"><get name="ds" /></map>
                            </set>
                        </think>
	                    <srai>QUESTION</srai>
	                </li>
